<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="manager" namespace="/" extends="struts-default">
		<action name="manager_*" class="managerAction" method="{1}">
			<result name="loginSuccess">index.jsp</result>
			<result name="login">/view/Login/login.jsp</result>
		</action>
	</package>
	
	<package name="merchant" namespace="/" extends="struts-default">
		<action name="merchant_*" class="merchantAction" method="{1}">
			<result name="add">/view/success.jsp</result>
		</action>
	</package>
</struts>